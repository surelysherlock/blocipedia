<h1>The Wealth of Knowledge from all Wikis lies before You</h1>

<%= link_to "New", new_wiki_path(@wiki), class: 'btn btn-sq-sm btn-success' %>

<% user = current_user %>

<% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <p><%= link_to wiki.title, wiki_path(wiki.id) %></p>
      </h4>
    </div>
  </div>
<% end %>
